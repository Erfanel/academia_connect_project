<!DOCTYPE html>
<html>
	<head>
		<meta charset="UTF-8">
		<title>
			{% block title %}!
			{% endblock %}
		</title>
		<link
		rel="icon" type="image/x-icon" href="{{ asset('images/logo_2.png') }}">


		{# css #}
		{% block stylesheets %}
			<link href="{{ asset('styles/app.css') }}" rel="stylesheet">
			<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">

		{% endblock %}


		{# bootstrap #}
		{% block javascripts %}
			<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz" crossorigin="anonymous"></script>

		{% endblock %}

		{# google font  #}
		<link rel="preconnect" href="https://fonts.googleapis.com">
		<link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
		<link href="https://fonts.googleapis.com/css2?family=Lato:ital,wght@0,100;0,300;0,400;0,700;0,900;1,100;1,300;1,400;1,700;1,900&display=swap" rel="stylesheet">
	</head>
	<body class="bg-dark text-dark" >
		<main class="bg-warning-subtle text-dark"> {% block body %}{% endblock %}
			</main>
			<footer class="d-flex px-5 py-2 justify-content-between bg-dark">
				
					<img src="{{ asset('images/Logo_2_Blanc.png') }}" alt="logo blanc" width="40" height="40">
					<p class="text-light">Tous droits réservés - 2024</p>
					<a href="mailto:email@example.com">Contacts & maintenance</a>
	
		
			</footer>
		</body>
	</html>
